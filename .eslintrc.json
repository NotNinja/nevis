{
  "extends": "skelp/v3/es5",
  "env": {
    "node": true
  },
  "rules": {
    "func-names": "off",
    "no-bitwise": "off",
    "no-empty-function": "off",
    "no-inline-comments": "off",
    "no-invalid-this": "off",
    "no-unused-vars": [
      "warn",
      {
        "args": "none"
      }
    ],
    "no-use-before-define": [
      "error",
      {
        "functions": false
      }
    ],
    "sort-keys": "off"
  }
}
